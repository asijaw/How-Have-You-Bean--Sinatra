<div class="coffee-header"><h2>All of <%= @user.username %>'s Coffees:</h2></div>
<div class="coffees-block">
<% @coffees.each do |coffee| %>
<div class="coffee">
<p><strong><a href="/coffees/<%= coffee.id %>"><%= coffee.name %></a></strong>, from <strong><a href="/coffees/roasters/<%= coffee.roaster.parameterize %>"><%= coffee.roaster %></a></strong>
</div>
<br>
<% end %> 
</div>